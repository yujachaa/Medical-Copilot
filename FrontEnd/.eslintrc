{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "no-var": "error", // var 금지
    "no-multiple-empty-lines": "error", // 여러 줄 공백 금지
    "eqeqeq": "error", // 일치 연산자 사용 필수
    "dot-notation": "error", // 가능하다면 dot notation 사용
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}